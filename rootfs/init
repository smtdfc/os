#!/bin/busybox sh

# Mount c√°c th∆∞ m·ª•c ·∫£o c·∫ßn thi·∫øt cho shell
mount -t proc none /proc
mount -t sysfs none /sys
mount -t devtmpfs none /dev || mknod -m 622 /dev/console c 5 1

# In ra ch√∫t cho vui
echo "[+] Welcome to smtdfcOS ü§°"
echo "[+] Starting BusyBox shell..."

# Ch·∫°y shell
exec /bin/busybox sh
